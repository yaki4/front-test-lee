{
  "version": 2,
  "builds": [
      {
          "src": "nuxt.config.js",
          "use": "@nuxtjs/vercel-builder",
          "config": {
            "serverFiles": ["api/**", "static/**", "assets/**"]
          }
      }
  ],
  "rewrites": [
    {
      "source": "/api/:match*",
      "destination": "https://front-test-lee.vercel.app//:match*"
    }
  ]
}